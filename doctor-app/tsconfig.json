{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/PA.jsx",
    "app/(tabs)/Dashboard.jsx",
    "app/_layout.jsx",
    "app/modal.jsx",
    "nativewind-env.d.ts"
  ]
}
